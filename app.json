{
  "expo": {
    "splash": {
      "image": "./assets/images/splashscreens/launch-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.xepiq.harmonyhive"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.xepiq.harmonyhive",
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"]
      },
      "buildNumber": "1.0.1"
    },
    "name": "Harmony Hive",
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/images/icon/harmonyhive/harmony-hive-icon.png",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "0a6a7fd9-7043-4700-9587-c03f4f949feb"
      }
    }
  }
}
